<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="scene5" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/17" fx:controller="com.example.hellofx.controllers.ControllerScene5">
    <children>
        <MenuBar layoutX="-1.0" prefHeight="26.0" prefWidth="600.0" styleClass="menuBar"
                 AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
            <menus>
                <Menu fx:id="fileMenu" mnemonicParsing="false" styleClass="menu" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" text="New"/>
                        <MenuItem mnemonicParsing="false" text="Save"/>
                        <CheckMenuItem mnemonicParsing="false" text="Autosave"/>
                        <SeparatorMenuItem mnemonicParsing="false"/>
                        <MenuItem mnemonicParsing="false" styleClass="menuItem" text="Close"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" styleClass="menu" text="Scenes">
                    <items>
                        <MenuItem fx:id="menuItemLogin" mnemonicParsing="false" onAction="#getToLogin"
                                  styleClass="menuItem"
                                  text="Login"/>
                        <MenuItem fx:id="menuItemScene2" mnemonicParsing="false" onAction="#getToScene2"
                                  styleClass="menuItem" text="Scene 2"/>
                        <MenuItem fx:id="menuItemScene3" mnemonicParsing="false" onAction="#getToScene3"
                                  styleClass="menuItem" text="Scene 3"/>
                        <MenuItem fx:id="menuItemScene4" mnemonicParsing="false" onAction="#getToScene4"
                                  styleClass="menuItem" text="Scene 4"/>
                        <MenuItem fx:id="menuItemScene5" mnemonicParsing="false" onAction="#getToScene5"
                                  styleClass="menuItem" text="Scene 5"/>
                        <MenuItem fx:id="menuItemScene6" mnemonicParsing="false" onAction="#getToScene6"
                                  styleClass="menuItem" text="Scene 6"/>
                        <MenuItem fx:id="menuItemScene7" mnemonicParsing="false" onAction="#getToScene7"
                                  styleClass="menuItem" text="Scene 7"/>
                        <MenuItem fx:id="menuItemScene8" mnemonicParsing="false" onAction="#getToScene8"
                                  styleClass="menuItem" text="Scene 8"/>
                        <MenuItem fx:id="menuItemScene9" mnemonicParsing="false" onAction="#getToScene9"
                                  styleClass="menuItem" text="Scene 9"/>
                        <MenuItem fx:id="menuItemScene10" mnemonicParsing="false" onAction="#getToScene10"
                                  styleClass="menuItem" text="Scene 10"/>
                        <MenuItem fx:id="menuItemScene11" mnemonicParsing="false" onAction="#getToScene11"
                                  styleClass="menuItem" text="Scene 11"/>
                        <MenuItem fx:id="menuItemScene12" mnemonicParsing="false" onAction="#getToScene12"
                                  styleClass="menuItem" text="Scene 12"/>
                        <MenuItem fx:id="menuItemScene13" mnemonicParsing="false" onAction="#getToScene13"
                                  styleClass="menuItem" text="Scene 13"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" styleClass="menu" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" styleClass="menuItem" text="About"/>
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <Button fx:id="btnBack" layoutX="61.0" layoutY="329.0" mnemonicParsing="false" onAction="#onBackButtonClick"
                text="Back"/>
        <Button fx:id="btnNext" layoutX="118.0" layoutY="329.0" mnemonicParsing="false" onAction="#onNextButtonClick"
                text="Next"/>
        <Button fx:id="btnLogout" layoutX="490.0" layoutY="329.0" mnemonicParsing="false"
                onAction="#onLogoutButtonClick"
                text="Logout"/>
        <ColorPicker fx:id="myColoPicker" layoutX="39.0" layoutY="36.0" onAction="#changeColor"/>
        <Label fx:id="myLabel" layoutX="269.0" layoutY="35.0" text="Displayed Colour">
            <font>
                <Font name="System Bold Italic" size="19.0"/>
            </font>
        </Label>
        <ChoiceBox fx:id="myChoice" layoutX="39.0" layoutY="106.0" prefWidth="150.0"/>
        <Label fx:id="brandLabel" layoutX="269.0" layoutY="106.0" text="Select a brand">
            <font>
                <Font size="17.0"/>
            </font>
        </Label>
        <Slider fx:id="vertSlider" layoutX="80.0" layoutY="147.0" majorTickUnit="10.0" orientation="VERTICAL"
                prefHeight="150.0" prefWidth="42.0" showTickLabels="true" showTickMarks="true" value="50.0"/>
        <Label fx:id="tempLabel" layoutX="269.0" layoutY="207.0" text="Temperature: ">
            <font>
                <Font size="20.0"/>
            </font>
        </Label>
    </children>

</AnchorPane>
